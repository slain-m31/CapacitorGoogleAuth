{
  "name": "@southdevs/capacitor-google-auth",
  "version": "7.0.0",
  "description": "Google Auth plugin for capacitor.",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "prepare": "tsc",
    "test": "echo \"no test specified\"",
    "docgen": "docgen --api GoogleAuthPlugin --output-readme README.md",
    "deploy": "npm run build && npm publish --access=public"
  },
  "author": "SouthDevs",
  "license": "MIT",
  "devDependencies": {
    "@capacitor/android": "^7.0.0",
    "@capacitor/cli": "^7.0.0",
    "@capacitor/core": "^7.0.0",
    "@capacitor/docgen": "^0.3.0",
    "@capacitor/ios": "^7.0.0",
    "@ionic/prettier-config": "^4.0.0",
    "@types/gapi": "0.0.42",
    "@types/gapi.auth2": "0.0.56",
    "prettier": "^3.4.2",
    "typescript": "^4.7.4",
    "prettier-plugin-java": "^2.6.6"
  },
  "peerDependencies": {
    "@capacitor/core": ">=7.0.0"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "!android/src/androidTest",
    "!android/src/test",
    "SouthDevsCapacitorGoogleAuth.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "google",
    "auth",
    "firebase",
    "angular",
    "vue",
    "react",
    "ionic"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devatsouth/CapacitorGoogleAuth"
  },
  "bugs": {
    "url": "https://github.com/devatsouth/CapacitorGoogleAuth/issues"
  },
  "prettier": "@ionic/prettier-config"
}
